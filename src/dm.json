  "browse": {
    "*": {
      "treetype": ["folder"],
      "list": {}
    },
    "user": {
      "treetype": ["folder"],
      "list": {}
    },
    "image": {
      "treetype": ["folder"],
      "list": {"level":0, "columns":["name"],"block_fields":["image","name"], "viewmode":"block", "pagination": 10}
    },
    "article": {
      "treetype": ["folder"],
      "list": {
